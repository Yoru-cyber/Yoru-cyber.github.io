---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/Hero.astro";
import * as md from "../content/cv.md"
import {Content as URLShortenerContent} from "./Projects/url-shortener.md";
import * as URLShortener from "./Projects/url-shortener.md"
import {Content as CurrencyAPIContent} from "./Projects/CurrencyAPI.md";
import * as CurrencyAPI from "./Projects/CurrencyAPI.md"
---

<Layout title="Yoru-cyber">
  <main class="flex flex-col items-center justify-center mt-10">
    <Hero name={md.frontmatter.name} location={md.frontmatter.location + ' 🇻🇪'} position={md.frontmatter.position + ' 🧑‍🎓'} description={md.frontmatter.description} />
  <section id="Projects" class="mt-48">
	<article class="flex flex-col justify-center items-center">
        <h1 class="text-4xl font-bold">Here are some projects i have made ✅</h1>
        <ul class="flex flex-col p-5">
            <li class="m-5">
                <a href="https://github.com/Yoru-cyber/URL-Shortener" class="text-2xl font-medium">{URLShortener.frontmatter.title}</a>
                <URLShortenerContent/>
            </li>
            <li class="m-5">
                <a href="https://github.com/Yoru-cyber/CurrencyAPI" class="text-2xl font-medium">{CurrencyAPI.frontmatter.title}</a>
                <CurrencyAPIContent/>
            </li>
        </ul>
    </article>
  </section>
  <section id="Contact">

      <article class="flex justify-center items-center">
          <h1>This about</h1>
      </article>
        
  </section>
</main>
</Layout>
